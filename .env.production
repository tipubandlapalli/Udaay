# Production Environment Variables Template
# Copy this to .env.production.local and fill in actual values
# NEVER commit .env.production.local to git!

# MongoDB
MONGO_URI=your_mongodb_connection_string

# JWT
JWT_SECRET=your_production_jwt_secret_min_32_chars

# Google Cloud
GOOGLE_CLOUD_PROJECT_ID=your_project_id
GOOGLE_CLOUD_BUCKET_NAME=your_bucket_name
GOOGLE_MAPS_API_KEY=your_maps_api_key

# Gemini AI
GEMINI_PROJECT_ID=your_gcp_project
GEMINI_LOCATION=us-central1
GEMINI_MODEL=gemini-1.5-flash-002

# Internal Service Authentication
INTERNAL_JWT_SECRET=civicfix_internal_service_secret_key_2026_secure_min_32_chars_long
